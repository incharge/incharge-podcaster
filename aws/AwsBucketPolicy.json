{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Action": [
				"s3:GetObject",
				"s3:PutObject",
				"s3:DeleteObject"
			],
			"Resource": [
				"arn:aws:s3:::thedissenter-episode/*",
				"arn:aws:s3:::thedissenter-transcript/*",
				"arn:aws:s3:::thedissenter-dev-episode/*",
				"arn:aws:s3:::thedissenter-dev-transcript/*",
				"arn:aws:s3:::thedissenter-test-episode/*",
				"arn:aws:s3:::thedissenter-test-transcript/*"
			],
			"Effect": "Allow"
		},
		{
			"Action": [
				"s3:ListBucket"
			],
			"Resource": [
				"arn:aws:s3:::thedissenter-episode",
				"arn:aws:s3:::thedissenter-transcript",
				"arn:aws:s3:::thedissenter-dev-episode",
				"arn:aws:s3:::thedissenter-dev-transcript",
				"arn:aws:s3:::thedissenter-test-episode",
				"arn:aws:s3:::thedissenter-test-transcript"
			],
			"Effect": "Allow"
		}
	]
}